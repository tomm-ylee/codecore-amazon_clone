<h1> Contact Us </h1>

<%= form_tag contact_path do %>
  <div>
    <%= label_tag :name %>
    <br/>
    <%= text_field_tag :name %>
  </div>
  <div>
    <%= label_tag :email %>
    <br/>
    <%= email_field_tag :email %>
  </div>
  <div>
    <%= label_tag :comment %>
    <br/>
    <%= text_area_tag :comment %>
  </div>
  <%= submit_tag 'Contact Us' %>

<% end %>
